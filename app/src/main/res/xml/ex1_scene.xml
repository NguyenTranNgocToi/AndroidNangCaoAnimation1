<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnSwipe motion:dragDirection="dragDown" />
    </Transition>



    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/view8"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="412dp"
            android:layout_height="0dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:visibility="invisible" />
        <Constraint
            android:layout_height="3dp"
            motion:layout_constraintStart_toStartOf="@+id/view8"
            motion:layout_constraintTop_toTopOf="@+id/view8"
            motion:layout_constraintHorizontal_bias="0.476"
            motion:layout_constraintEnd_toEndOf="@+id/view8"
            android:layout_width="374dp"
            android:id="@+id/textView4"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/imageView12"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="25dp"
            android:layout_height="45dp"
            android:layout_marginEnd="16dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp" />
        <Constraint
            android:id="@+id/textView"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="325dp"
            android:layout_height="45dp"
            motion:layout_constraintHorizontal_bias="0.581"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="640dp">
            <CustomAttribute
                motion:attributeName="textColor"
                motion:customColorValue="#ffffff" />
        </Constraint>
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView"
            android:layout_marginTop="16dp"
            android:layout_marginStart="16dp"
            android:visibility="invisible" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="45dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="56dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.488"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="325dp"
            android:id="@+id/textView" >
            <CustomAttribute
                motion:attributeName="textColor"
                motion:customColorValue="#2542E3" />
        </Constraint>
        <Constraint
            android:layout_height="60dp"
            android:layout_marginTop="652dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="60dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="12dp"
            android:id="@+id/imageView11"
            android:layout_marginLeft="12dp" />
        <Constraint
            android:layout_marginEnd="50dp"
            android:layout_height="20dp"
            android:layout_marginBottom="24dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="259dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="50dp"
            android:id="@+id/imageView13"
            motion:layout_constraintHorizontal_bias="0.711" />
        <Constraint
            android:layout_marginEnd="16dp"
            android:layout_height="50dp"
            android:layout_marginTop="656dp"
            android:layout_marginRight="16dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="50dp"
            android:id="@+id/imageView12" />
        <Constraint
            android:layout_height="28dp"
            android:layout_marginBottom="44dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="28dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="92dp"
            android:layout_marginStart="92dp"
            android:id="@+id/imageView14" />
        <Constraint
            android:layout_height="28dp"
            android:layout_marginBottom="44dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="28dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="160dp"
            android:layout_marginStart="160dp"
            android:id="@+id/imageView15" />
        <Constraint
            android:layout_height="28dp"
            android:layout_marginBottom="44dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="28dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="232dp"
            android:layout_marginStart="232dp"
            android:id="@+id/imageView16" />
        <Constraint
            android:layout_marginEnd="88dp"
            android:layout_height="28dp"
            android:layout_marginRight="88dp"
            android:layout_marginBottom="44dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="28dp"
            android:id="@+id/imageView17" />
        <Constraint
            android:layout_height="459dp"
            motion:layout_constraintStart_toStartOf="@+id/view8"
            android:layout_marginTop="144dp"
            motion:layout_constraintTop_toTopOf="@+id/view8"
            motion:layout_constraintHorizontal_bias="0.404"
            motion:layout_constraintEnd_toEndOf="@+id/view8"
            android:layout_width="380dp"
            android:id="@+id/textView4" />
        <Constraint
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteY="16dp"
            motion:layout_editor_absoluteX="95dp"
            android:visibility="visible" />
        <Constraint
            android:id="@+id/imageView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp"
            android:visibility="invisible" />
    </ConstraintSet>
</MotionScene>